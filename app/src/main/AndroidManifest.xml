<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.strzelecka_18">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Strzelecka 18"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Strzelecka_18">
        <activity android:name=".StartersActivity"
            android:label="Starters"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".MainCoursesActivity"
            android:label="Main Courses"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".DessertsActivity"
            android:label="Desserts"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>